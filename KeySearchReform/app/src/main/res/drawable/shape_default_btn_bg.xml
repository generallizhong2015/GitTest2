<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_focused="true">
        <shape>
            <corners android:radius="9.0dip" />
            <solid android:color="@color/bule_03" />
        </shape>
    </item>
    <item android:state_pressed="false">
        <shape>
            <corners android:radius="9.0dip" />
            <solid android:color="@color/bule_05" />
        </shape>
    </item>

    <item
        android:bottom="1dp"
        android:left="1dp"
        android:right="1dp"
        android:top="1dp">
        <shape>
            <gradient
                android:angle="90"
                android:centerColor="#1C86EE"
                android:centerX="0.5"
                android:centerY="0.5"
                android:endColor="#1874CD"
                android:startColor="#6495ED"
                android:type="linear" />
            <padding
                android:bottom="2dp"
                android:left="2dp"
                android:right="2dp"
                android:top="2dp" />
            <corners
                android:bottomLeftRadius="0dip"
                android:bottomRightRadius="0dip"
                android:topLeftRadius="5dip"
                android:topRightRadius="5dip" />
        </shape>
    </item>


</layer-list>